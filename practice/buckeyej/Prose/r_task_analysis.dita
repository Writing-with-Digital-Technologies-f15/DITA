			<proptype>Save</proptype>
        		<propvalue>Disk Button on Right-hand column</propvalue>
        	<propdesc>The button will have a highlighted corner when a change has been made which is eligible to be saved.
      		</propdesc>
		</property>
      		<property>
        	<proptype>Headings</proptype>
        	<propvalue>#, ##, ###</propvalue>
        	<propdesc>The more symbols preceding a heading, the smaller it will be</propdesc>
      		</property>
      		<property>
        	<proptype>Link</proptype>
        	<propvalue>Hyperlink Button on the toolbar</propvalue>
        	<propdesc>Specify the site to which the highlighted text will link</propdesc>
      		</property>
	</properties>
	</section>
